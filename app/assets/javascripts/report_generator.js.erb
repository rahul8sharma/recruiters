//= require ajax

$(document).ready(function(){

  function newAssessment(){
    $("#new-assessment").show();
  }
  function loadAssessment(){
    alert("Loading Assessment");
  }




  $("#report_assessment_new").on("click",function(){
    newAssessment();
  });

  $("#report_assessment_existing").on("click",function(){
    loadAssessment();
  });


});