= form_for :item, :url => import_with_options_from_google_drive_objective_items_path, html: { multipart: true }, :method => :post do |f|
  %fieldset
    %legend
      Import NEW Objective Items alongwith Options
    %br
    %br
    = f.fields_for :file do |ff|
      File
      = ff.text_field "url", value: ""
    = f.hidden_field :email, :value => current_user.email
    %br
    %br
    .actions
      = submit_tag 'Import Objective Items'
%br

%br/
= render :partial => "master_data/export_form"

%br/
= render :partial => "master_data/import_from_google_drive"

= link_to 'Back', objective_items_path
=# link_to 'Destroy All', destroy_all_objective_items_path
