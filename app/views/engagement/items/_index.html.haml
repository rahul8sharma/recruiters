%table
  %tr
    %th Id
    %th Body
    %th Element ID
    %th Active
    %th Edit
  - items.each do |item|
    %tr
      %td{ width: "5%" }
        = link_to item.id, engagement_item_path(item.id)
      %td{ width: "30%" }
        = link_to item.body, engagement_item_path(item.id)
      %td{ width: "10%" }
        = item.element_id
      %td{ width: "10%" }
        = item.active
      %td{ width: "20%" }
        = link_to "Edit", edit_engagement_item_path(item.id)

