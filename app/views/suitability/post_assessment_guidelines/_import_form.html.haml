= form_tag import_via_s3_suitability_post_assessment_guidelines_path, multipart: true, :method => :post do
  %fieldset
    %legend 
      %strong Upload new suitability post_assessment_guidelines
    = fields_for "import" do |f|
      %p
        Zipped CSV File
        = f.file_field "file"
      %p
        Notification Email : 
        = f.text_field :email, :value => "product@jombay.com"

    .actions
      = submit_tag 'Upload Suitability Post Assessment Guidelines'
