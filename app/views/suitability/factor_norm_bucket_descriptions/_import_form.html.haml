= form_tag import_via_s3_suitability_factor_norm_bucket_descriptions_path, multipart: true, :method => :post do
  %fieldset
    %legend 
      %strong Upload new suitability factor norm bucket descriptions
    = fields_for "import" do |f|
      %p
        Zipped CSV File
        = f.file_field "file"
      %p
        Notification Email : 
        = f.text_field :email, :value => "product@jombay.com"

    .actions
      = submit_tag 'Upload Suitability factor norm bucket descriptions'
