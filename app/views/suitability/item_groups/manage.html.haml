= form_for :item_group, :url => import_with_options_from_google_drive_suitability_item_groups_path, html: { multipart: true }, :method => :post do |f|
  %fieldset
    %legend
      Import Suitability Items
    %br
    %br
    = f.fields_for :file do |ff|
      Google Spreadsheet URL
      = ff.text_field "url", value: ""
    %br
    %br  
    = f.fields_for :zip_file do |ff|
      Image Zip
      = file_field_tag "zip"
    %br
    %br
    Notifier Email
    = f.email_field :email, value: "product@jombay.com"
    %br
    %br
    .actions
      = submit_tag 'Import Suitability Items'
%br
%br/
= render :partial => "master_data/#{controller.import_from}"
%br/
= render :partial => "master_data/export_form"
%br/
= link_to 'Back', suitability_item_groups_path     
=# link_to 'Destroy All', destroy_all_suitability_item_groups_path     
