%h1 #{controller.resource_name.humanize}

= link_to "Manage #{controller.resource_name.humanize}", controller.manage_path

= render :partial => "master_data/search"

.pagination.pull-right
  = paginate @objects
.clr  

%table
  %tr
    - controller.index_columns.each do |column|
      %th
        = column.to_s.humanize
    %th
      Edit    
  - @objects.each do |object|
    %tr 
      - controller.index_columns.each do |column|
        %td
          = object.send(column)
      %td
        = link_to "Edit", edit_suitability_competency_path(object)

.pagination.pull-right
  = paginate @objects
.clr   
