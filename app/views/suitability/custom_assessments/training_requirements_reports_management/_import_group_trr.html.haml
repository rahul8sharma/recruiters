= form_tag import_group_trr_users_path, multipart: true, :method => :post do
  %fieldset
    %legend
      %strong Import Candidate List(Multiple Assessments - Group TRR)
    %p
      TRR ID :
      = text_field_tag :assessment_group_id

    %p
      = fields_for :import do |ff|
        XLS File
        = ff.file_field "file"

  .actions
    = submit_tag 'Regenerate TRR'
