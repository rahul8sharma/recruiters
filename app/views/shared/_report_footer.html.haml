- force ||= false
- navigation ||= true
- last_page ||= false
- if @view_mode != "html" || force
  .footer.clearfix
    - if @view_mode != "html" && navigation
      .medium-text.pull-left
        = render :partial => "assessment_reports/navigation.html.haml", :locals => { :current_page => current_page }
      .left-offset1.pull-left
        Private & Confidential
    - else    
      .pull-left
        Private & Confidential
    .medium-text.pull-right 
      &copy; Jombay  
- if @view_mode == "html" && !last_page     
  %hr
