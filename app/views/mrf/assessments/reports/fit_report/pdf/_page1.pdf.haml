.overall_report
  .divider1
  .j-logo.pull-left
  .clr
  .divider28
  .huge-text.left-offset1
    = @report.report_hash[:candidate][:name]
  .divider1
  .banner
    .banner-heading
      360&#176;
    .banner-sub-heading.pull-left
      Profiling Report
    = render :partial => "mrf/assessments/reports/fit_report/pdf/mrf_report_numerical_aggregate_score"
    = render :partial => "mrf/assessments/reports/fit_report/pdf/mrf_report_show_completion_status"
  .divider2
  .extra-large-text
    .span.left-offset1
      360&#176; Profiling:
    .span13
      = @report.report_hash[:assessment][:name]
    .clr
    .divider2
    .span.left-offset1
      Report Generated On:
    .span
      = hdate @report.report_hash[:generated_at]
    .clr