- @report.report_data[:subjective_responses_others].each do |subjective_response_key, subjective_response_value|
  .bottom-trangle
    .frame
      .question
        = subjective_response_value[:body]
        .info
          = subjective_response_value[:trait_name].present? ? "This question was about the trait #{subjective_response_value[:trait_name]}." : ""
          = subjective_response_value[:compulsory] ? "Response to question was Mandatory" : "Response to question was Optional"
        .arrow
      .divider2
      - section_value.each do |section|
        = render :partial => section["path"], :locals => {:subjective_response_value => subjective_response_value }
  .divider2

