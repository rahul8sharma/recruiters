.container#manager_guideline
  .divider2
  .title
    Summary of 360 Degree Feedback
  .divider2
.overview
  .top-triangle
    .bottom-triangle
      .container
        .feedback-stats
          .divider2
          .caption
            Feedback Stats
          %ul
            %li.head
              .left
                Feedback from
              .middle
                Requests to
              .right
                Completed by
              .clr
            - @report.report_data[:feedback_stats].each do |feedback_name, feedback_stats|
              %li.list
                .left
                  = feedback_name.titleize
                .middle
                  = feedback_stats[:sent]
                .right
                  = feedback_stats[:completed]
                .clr
        .divider2