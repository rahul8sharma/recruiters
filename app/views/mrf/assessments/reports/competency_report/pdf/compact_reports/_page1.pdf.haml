.overall_report
  .divider1
  .j-logo.pull-left
  - if @report.report_data[:company][:logo_url].present?
    - logo_url = @report.report_data[:company][:logo_url]
    .pull-left{ style: "width: 50px;" }
      = wicked_pdf_image_tag(logo_url, :style => "padding: 5px 0 0 10px;")
  .clr
  .divider28
  .huge-text.left-offset1
    = @report.report_hash[:user][:name]
  .divider1
  .banner
    .banner-heading.pull-left
      #{get_assessment_purpose(@report)}&#176;
    .pull-right
      .divider3
      .banner-sub-heading
        Profiling Report
      - section_value.each do |section|
        = render :partial => section["path"]
    .clr
  .divider2
  .extra-large-text
    .span.left-offset1
      #{get_assessment_purpose(@report)}&#176; Profiling:
    .span13
      = @report.report_hash[:assessment][:name]
    .clr
    .divider2
    .span.left-offset1
      Exercise Completed On:
    .span
      = hdate @report.report_hash[:generated_at]
    .clr
.page-break
