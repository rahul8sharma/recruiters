- if @report.report_data[:mrf_report_show_trait_comments] && @report.report_data[:comments_enabled]
  - @report.report_data[:competency_scores].each do |competency_name, competency_scores|
    - competency_scores[:trait_scores].each do |trait_scores|
      = render partial: "shared/360/compact_mrf_report_header.pdf.haml"
      .stakeholder-comments.responses
        = render :partial => "mrf/assessments/reports/competency_report/pdf/compact_reports/comments_on_traits", :locals => { :competency_name => competency_name, :competency_scores => competency_scores, trait_scores: trait_scores}
      .page-break
