- if @report.report_data[:competency360_competencies_visual_data_points] && competency_scores[:role_wise_scores].present?
  -#Average Stakholder Rating section
  .scores_competencies_graph.descending_order.descending_overall_score.descending_average_rating
    .traits_under_graph
      .small-text.bold
        Average rating given by Stakeholders
      .clr
      
      .graph_container
        .left_sec.pull-left
          .divider2
          %ul
            - competency_scores[:role_wise_scores].each do |role, role_wise_score|
              %li
                .span3.small-text.first.trait_title
                  = role.pluralize.titleize
                  
                .stakeholder_rating.small-text.pull-left
                  = role_wise_score[:points]
                .clr


        .right_sec.span15.pull-right.first
          .classic_competency_graph
            .classic_competency_graph_scale
              .block.pull-left
                = @competency_graph_buckets.first.min_val
              - @competency_graph_buckets[1..@competency_graph_buckets.length-1].each do |competency_graph_bucket|
                .block.pull-left{:style => "width: #{590/(@competency_graph_buckets.size-1)}px; text-align: right"}
                  = competency_graph_bucket.min_val
              
              .clr
            .graph
              .graph_content
                - old_min = @competency_graph_buckets.first.min_val
                - old_max = @competency_graph_buckets.last.max_val
                - competency_scores[:role_wise_scores].each do |role, role_wise_score|  
                  - if role_wise_score[:points]
                    - score = role_wise_score[:points]
                    - percent = convertToRange(old_min, old_max, 0, 100, score)
                    - percent = percent == 0 ? 2 : percent
                  - else
                    - percent = 0
                  .divider1
                  .descending_graph_bar.max.no-margin{:style => "width: #{percent}%; background: #6ea4be"}
                  .divider1
            .divider2    
        .clr 
