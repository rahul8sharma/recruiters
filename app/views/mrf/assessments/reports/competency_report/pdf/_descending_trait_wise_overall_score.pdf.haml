- if @report.report_data[:competency360_traits_visual_data_points]  
  -#Overall Score section
  .scores_competencies_graph.descending_order.descending_overall_score.border-bottom-grayLighter
    .traits_under_graph
      .divider1
      .small-text.bold
        Overall Score
      .clr
      
      .graph_container
        .left_sec.pull-left
          .divider2
          %ul
            %li
              .span3.small-text.first.trait_title
                360&#176 Score
                
              .stakeholder_rating.small-text.pull-left
                = trait_scores[:average_score_excluding_self][:points]
              .clr

            %li
              .span3.small-text.first.trait_title
                Self Rating
                
              .self_rating.small-text.pull-left
                = trait_scores[:average_score_self][:points]
              .clr

            %li
              .span3.small-text.first.trait_title
                360&#176 (Including Self)
                
              .including_self_rating.small-text.pull-left
                = trait_scores[:average_score_including_self][:points]
              .clr

        .right_sec.span15.pull-right.first
          .classic_competency_graph
            .classic_competency_graph_scale
              .block.pull-left
                = @trait_graph_buckets.first.min_val
              - @trait_graph_buckets[1..@trait_graph_buckets.length-1].each do |trait_graph_bucket|
                .block.pull-left{:style => "width: #{590/(@trait_graph_buckets.size-1)}px; text-align: right"}
                  = trait_graph_bucket.min_val
              
              .clr
              
              .clr
            .graph
              .graph_content
               
                - old_min = @trait_graph_buckets.first.min_val
                - old_max = @trait_graph_buckets.last.max_val
                - if trait_scores[:average_score_excluding_self][:points]
                  - score = trait_scores[:average_score_excluding_self][:points]
                  - percent = convertToRange(old_min, old_max, 0, 100, score)
                  - percent = percent == 0 ? 2 : percent
                - else  
                  - percent = 0  
                
                .divider1
                .descending_graph_bar.max.no-margin{:style => "width: #{percent}%; background: #ffcf36"}
                .divider1
                - if trait_scores[:average_score_self][:points]
                  - score = trait_scores[:average_score_self][:points]
                  - percent = convertToRange(old_min, old_max, 0, 100, score)
                  - percent = percent == 0 ? 2 : percent
                - else
                  - percent = 0
                
                .descending_graph_bar.max{:style => "width: #{percent}%; background: #a729ac"}
                .divider1
                - score = trait_scores[:average_score_including_self][:points]
                - percent = convertToRange(old_min, old_max, 0, 100, score)
                - percent = percent == 0 ? 2 : percent
                .descending_graph_bar.max{:style => "width: #{percent}%; background: #abed74"}
                .divider1
            .divider1   
        .clr  
        
        -#Overall Score max/min section

        .left_sec.pull-left
          .divider2
          %ul
            %li
              .span3.small-text.first.trait_title
                Minimum Rating
                
              .min_rating.small-text.pull-left
                = trait_scores[:min_score_including_self][:points]
              .clr
            %li
              .span3.small-text.first.trait_title
                Maximum Rating
                
              .max_rating.small-text.pull-left
                = trait_scores[:max_score_including_self][:points]
              .clr

        .right_sec.span15.pull-right.first
          .classic_competency_graph
            .classic_competency_graph_scale
              .block.pull-left
                = @trait_graph_buckets.first.min_val
              - @trait_graph_buckets[1..@trait_graph_buckets.length-1].each do |trait_graph_bucket|
                .block.pull-left{:style => "width: #{590/(@trait_graph_buckets.size-1)}px; text-align: right"}
                  = trait_graph_bucket.min_val
              
              .clr
              
              .clr
            .graph
              .graph_content
               
                .divider1
                - score = trait_scores[:min_score_including_self][:points]
                - percent = convertToRange(old_min, old_max, 0, 100, score)
                - percent = percent == 0 ? 2 : percent
                .descending_graph_bar.max.no-margin{:style => "width: #{percent}%; background: #abed74"}
              
                .divider1
           
                - score = trait_scores[:max_score_including_self][:points]
                - percent = convertToRange(old_min, old_max, 0, 100, score)
                - percent = percent == 0 ? 2 : percent
                .descending_graph_bar.max{:style => "width: #{percent}%; background: #abed74"}
              
                .divider1
            .divider1    
        .clr
