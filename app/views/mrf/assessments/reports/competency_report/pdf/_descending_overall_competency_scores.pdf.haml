- if @report.report_data[:competency360_competencies_visual_data_points]  
  -#Overall Score section
  .scores_competencies_graph.descending_order.descending_overall_score.border-bottom-grayLighter
    .traits_under_graph
      .divider2
      .small-text.bold
        Overall Score
      .clr
      
      .graph_container
        .left_sec.pull-left
          .divider2
          %ul
            %li
              .span3.small-text.first.trait_title
                360&#176 Score
                
              .stakeholder_rating.small-text.pull-left
                = competency_scores[:score][:points] || "-"
              .clr

            %li
              .span3.small-text.first.trait_title
                Self Rating
                
              .self_rating.small-text.pull-left
                = competency_scores[:average_score_self][:points] || "-"
              .clr

            %li
              .span3.small-text.first.trait_title
                360&#176 (Including Self)
                
              .including_self_rating.small-text.pull-left
                = competency_scores[:average_score_including_self][:points] || "-"
              .clr

        .right_sec.span15.pull-right.first
          .classic_competency_graph
            .classic_competency_graph_scale
              .block.pull-left
                = @competency_graph_buckets.first.min_val
              - @competency_graph_buckets[1..@competency_graph_buckets.length-1].each do |competency_graph_bucket|
                .block.pull-left{:style => "width: #{590/(@competency_graph_buckets.size-1)}px; text-align: right"}
                  = competency_graph_bucket.min_val
              
              .clr
            .graph
              .graph_content
                - old_min = @competency_graph_buckets.first.min_val
                - old_max = @competency_graph_buckets.last.max_val
                - if competency_scores[:score][:points]
                  - score = competency_scores[:score][:points]
                  - percent = convertToRange(old_min, old_max, 0, 100, score)
                  - percent = percent == 0 ? 2 : percent
                - else
                  - percent = 0
                
                .divider1
                .descending_graph_bar.max.no-margin{:style => "width: #{percent}%; background: #a119a6"}
              
                - if competency_scores[:average_score_self][:points]
                  - score = competency_scores[:average_score_self][:points]
                  - percent = convertToRange(old_min, old_max, 0, 100, score)
                  - percent = percent == 0 ? 2 : percent
                - else
                  - percent = 0
                .divider1
                .descending_graph_bar.max{:style => "width: #{percent}%; background: #24d8ba"}
                
                - if competency_scores[:average_score_including_self][:points]
                  - score = competency_scores[:average_score_including_self][:points]
                  - percent = convertToRange(old_min, old_max, 0, 100, score)
                  - percent = percent == 0 ? 2 : percent
                - else
                  - percent = 0
                  
                .divider1
                .descending_graph_bar.max{:style => "width: #{percent}%; background: #c4a655"}
                .divider1
            .divider2    
        .clr  
