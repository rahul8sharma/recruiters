#scores.section03
  .divider1
  .scores.container
    .span10.title.pull-left.line-height2
      Competencies with 360&#176; Scores 
    .scale.mrf.pull-right.left-offset1
      .line
      .marker-container
        - @norm_buckets.each do |norm_bucket|
          .marker
            .arrow
            .marker-info
              = norm_bucket.description rescue "-" 
            - if norm_bucket.name.split(" ").size == 1
              %br/
            = norm_bucket.name
      .clr
    .clr
    .divider3
    .frame.mrf.gray-bg
      %ul.mrf_scores_listing
        - @report.report_data[:competency_scores].each do |competency_name, competency_scores|
          = render :partial => "mrf/assessments/reports/competency_report/competencies_aggregated.html", :locals => { :competency_name => competency_name, :competency_scores => competency_scores }
    .divider2