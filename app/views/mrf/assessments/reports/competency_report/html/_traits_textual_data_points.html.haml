#distribution_data_points.pull-left 
  .span.heading.medium-text.line-height1{:style => "margin-top: 3px; padding: 4px"}
    360&#176; Rating : #{trait_score[:average_score_excluding_self][:points]}
  .trait_rating.max.span{:style => "width: 150px"}
    .left
      Highest
    .right
      = @assessment.report_data[:trait_scores][trait_score[:trait][:name]][:max_score][:score] == 0 ? "-" : @assessment.report_data[:trait_scores][trait_score[:trait][:name]][:max_score][:score]
    .clr
  .trait_rating.min.span{:style => "width: 150px"}
    .left
      Lowest
    .right
      = @assessment.report_data[:trait_scores][trait_score[:trait][:name]][:min_score][:score] == 0 ? "-" : @assessment.report_data[:trait_scores][trait_score[:trait][:name]][:min_score][:score]
    .clr
  .clr

.clr

 