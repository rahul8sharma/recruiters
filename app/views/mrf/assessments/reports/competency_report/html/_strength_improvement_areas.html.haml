.strength_weakness_areas.section02
  .divider2
  .scores.container
    .title
      Strengths and Areas of Improvement
    .divider2
    - @report.report_data[:strengths_and_areas_of_improvement].each do |type, traits|
      .extra-large-text
        = type.titleize
      .divider1
      %table.section_description.extra-medium-text{:cellpadding=>"0", :cellspacing=>"0", :align=>"top"}
        %tbody
          %tr
            %td.left-info.big-text
              .padding-all1
                = Rails.application.config.reports["mrf_report"][type.to_s]
            %td.right-info
              .padding-all1
                - if traits.empty?
                  No such traits
                - else  
                  %ul.large-text
                    - traits.each do |trait|
                      %li
                        = trait
      .divider2

