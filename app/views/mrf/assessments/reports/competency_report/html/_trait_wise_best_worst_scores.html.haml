.classic_report
  .scores_competencies_graph.descending_order.descending_overall_score.best_worst_scores.trait_wise.border-bottom-grayLighter
    .traits_under_graph
      .divider2
      .large-text
        360&#176; Scores across exercise
      .divider1
      .graph_container
        .left_sec.pull-left
          .divider2
          %ul.default
            %li

              .span3.large-text.first.trait_title
                Lowest
              .span2.first.divider2.min_rating_icon_small
              .min_rating.medium-text.pull-left
                1.0
              .clr
            %li
              .span3.large-text.first.trait_title
                Candidate
              .span2.first.divider2.stakeholder_rating_icon_small
              .stakeholder_rating.medium-text.pull-left
                3.31
              .clr
            %li
              .span3.large-text.first.trait_title
                Highest
              .span2.first.divider2.max_rating_icon_small
              .max_rating.medium-text.pull-left
                2.73
              .clr
        .right_sec.span22.pull-right
          .classic_competency_graph
            .classic_competency_graph_scale
              .block.pull-left
                1.0
              - 4.times do |index|
                .block.pull-left{:style => "width: #{830/4}px; text-align: right"}
                  #{index+2}
              .clr
            .graph
              .graph_content
                .divider1
                .descending_graph_bar.max{:style => "width: 57.75%; background: #ffcf36"}
                .divider1
                .divider1
                .descending_graph_bar.max{:style => "width: 5%; background: #a729ac"}
                .divider1
                .divider1
                .descending_graph_bar.max{:style => "width: 43.25%; background: #abed74"}
                .divider1
        .clr