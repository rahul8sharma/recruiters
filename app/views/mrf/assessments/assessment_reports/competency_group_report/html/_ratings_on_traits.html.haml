                  .stakeholder_rating.medium-text.pull-left
                    3.09
                  .clr
             
          .right_sec.span22.pull-right
            .classic_competency_graph
              .classic_competency_graph_scale
                .block.pull-left
                  0
                - 5.times do |index|
                  .block.pull-left{:style=>"width: #{(830/5)}px; text-align: right"}
                    = index+1

                .clr
               
              .graph
                .graph_content
                  -5.times do
                    .divider1
                    .descending_graph_bar.max{:style => "width: 12%; background: #B100A0"}
                   
                    .divider1
          .clr