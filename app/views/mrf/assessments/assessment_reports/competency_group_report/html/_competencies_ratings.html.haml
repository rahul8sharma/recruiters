.classic_report
  .scores_competencies_graph.descending_order
    .divider2
    .container
      .title.pull-left
        360&#176; Ratings on Competencies/Values
      .graph_instructions.extra-medium-text.left-offset1.pull-left
        .stakeholder_rating_icon.span
        .span.bold
          360&#176; Rating
        .clr
      .clr
      .traits_under_graph
        .graph_container
          .left_sec.pull-left
            .graph_instructions.pull-right
              .span1.divider2.stakeholder_rating_icon
              .clr
            .clr
            %ul
              - 5.times do
                %li
                  .trait_title.first.span7.extra-medium-text.bold
                    competency
                  .stakeholder_rating.medium-text.pull-left
                    2.2
                  .clr
                
          .right_sec.span21.pull-right
            .classic_competency_graph
              .classic_competency_graph_scale
                .block.pull-left
                  0
                -4.times do |index|
                  .block.pull-left{:style=>"width: #{830/4}px; text-align: right"}
                    #{index+1}
                .clr
              .graph.no-background
                .graph_content.no-background
                  -5.times do
                    .divider1
                    .descending_graph_bar.max{:style => "width: 20%; background: #a119a6"}
                    .descending_graph_bar.min
                    .divider1
        .clr
    .divider2