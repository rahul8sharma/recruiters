    .title
      Distribution of Overall  360&#176; scores: Competencies

    .border-bottom-grayDark
      .span15.first
        .divider2
        .large-text.left-offset1
          Communication Skills
        .divider2
        .grid_graph.span13.left-offset1
          .columns
            -6.times do
              .column.span1.left-offset1{:style=>"background: #2b7bbe; margin-top: #{(300*70/100)}px; height: 30%"}
                .rating_range
                  0 - 1
            .clr

          .counts
            -counter = 110
            -11.times do |index|
              - counter = counter-10
              .count
                #{counter} 
        .clr

