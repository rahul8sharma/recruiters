= form_tag import_from_google_drive_companies_path, multipart: true, :method => :post do
  %fieldset
    %legend 
      %strong Upload new companies
    = fields_for "import" do |f|
      = f.fields_for "file" do |f|
        %p
          Google Spreadsheet Url : 
          = f.text_field "url"
        %p
          Worksheet Names(separated by comma,keep empty to import all)
          = f.text_field "worksheets"
      = f.hidden_field :email, :value => current_user.email

    .actions
      = submit_tag 'Upload companies'
