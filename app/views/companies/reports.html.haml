    Assessments
  .clr

  .divider2
  .list-snippet-titles.large-text.clearfix
    .span3.first
      %a{:href=>"#"}
        ATID
        %i.icon-chevron-down.small-text.line-height1
    .span7
      %a{:href=>"#"}
        Assessment Taker
        %i.icon-chevron-down.small-text.line-height1
    .span9
      %a{:href=>"#"}
        Assessment Title
        %i.icon-chevron-down.small-text.line-height1
    .span7.left-offset1
      %a{:href=>"#"}
        Completed on
        %i.icon-chevron-down.small-text.line-height1
    .span2
      %a{:href=>"#"}
        Report
    .clr
    .divider1          
  %ul.unstyled
    -10.times do |index|
      %li.list-snippet.border-bottom-grayLighter
