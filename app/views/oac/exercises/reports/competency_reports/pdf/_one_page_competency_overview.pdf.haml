- height_of_graph = ((@report.report_data[:super_competency_scores].size * 50)+120)
- page_height = 800
- remaining_space = height_of_graph > 800 ? 0 : page_height - height_of_graph
- description_size = 200

- text = ""
- @report.report_data[:super_competency_scores].each do |name , data|
  - text += data[:description]

- total_length = text.size
-# 1 line is 20px
- lines = (remaining_space/20 )

- Rails.logger.ap "TOTAL SIZE"
- Rails.logger.ap total_length

