.tool_scores
  .span2.text-left.no-border.small-text
    Score
  .pull-right
    - @report.report_data[:tools].each_with_index do |(tool, tool_data),index|
      - if toolwise_scores[tool]
        .span2.small-text{ class: (index==0? "no-border text-center":"") }
          -# = toolwise_scores[tool][:score_bucket]
          = @custom_score_bucket_names["#{toolwise_scores[tool][:uid]}"]
    .clr
  .clr
