= render partial: "shared/reports/pdf/oac_report_header.pdf.haml"
.table_of_contents
  .inner
    .blue_heading.big-text
      Table of Contents
    %hr
    .divider1
    %ul
      - @report.report_configuration[:pdf][:sections].select{|section| ['Cover Page','Table of Contents'].exclude?(section[:text]) }.each_with_index do |section, index|
        %li
          .section_name
            #{section[:text]}
          .page_no
            #{index+1}
          .clr