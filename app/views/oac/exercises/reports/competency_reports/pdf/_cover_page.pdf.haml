.overall_report
  .divider1
  .clr
  - if @report.report_data[:company][:logo_url]
    .divider6
    .span5
      - url = @report.report_data[:company][:logo_url]
      = image_tag(url)
    .clr
    .divider22
  - else
    .divider28
  .banner
    .banner-heading
      HiVE VDC Report
  .divider2
  .left-offset1
    - section_value.each do |value|
      = render :partial => value["path"], locals: {:section_value => value[:children]}
  .divider2
  .partner_logos.left-offset1
    - @report.report_data[:tools].map{|x,y| y[:logo].present? ? y[:logo] : nil }.compact.uniq.each do |logo|
      = image_tag("report/html/oac/partner_logos/#{logo}")
.page-break
