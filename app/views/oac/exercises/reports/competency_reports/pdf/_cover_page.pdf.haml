.overall_report
  .divider1
  .clr
  .divider28
  
  .banner
    .banner-heading
      HiVE VDC Report
  .divider2
  .left-offset1
    .extra-large-text
      = @report.report_data[:user][:name]
      .divider1
      = @report.report_data[:exercise][:name]
      .divider1
    .large-text
      .pull-left
        Report Generated On:
      .span
        = hdate @report.report_data[:completed_at]
      .clr
  .divider2
  .partner_logos.left-offset1
    - @report.report_data[:tools].map{|x,y| y[:logo].present? ? y[:logo] : nil }.compact.uniq.each do |logo|
      = image_tag("report/html/oac/partner_logos/#{logo}")

.page-break
