.scores_on_competencies
  .container
    .divider2
    .title
      Scores on Competencies
    .divider1

    .traits_under_graph
      .graph_container
        .left_sec.pull-left
          .graph_instructions.left-offset4
            .span1.divider2 
          %ul
            - 5.times do
              %li
                .trait_title.first.span8{:title => "Empathy"}
                  Empathy
                .clr
            
        .right_sec.span21.first.pull-right
          .classic_competency_graph
            .classic_competency_graph_scale
              .block.pull-left
                0
              - 4.times do |index|
                .block.pull-left{:style => "width: #{830/4}px;"}
                  #{index+1}

              .clr
            .graph
              .graph_content
                - 5.times do
                  .divider1
                  .descending_graph_bar.max{:style => "width: 60.0%;"}
                  .divider1
                  
        .clr
