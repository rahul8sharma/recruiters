= form_for :user, url: export_user_report_urls_users_path, multipart: true, :method => :post do |f|
  %input{ type: "hidden", name: "user[job_klass]", value: "Suitability::UserReportUrlsExporter" }
  %fieldset
    %legend 
      %strong Download Candidate Report Urls
    = f.fields_for "args" do |f|
      %p
        Assessment ID : 
        = f.text_field :assessment_id
      %p
        Notification Email : 
        = f.text_field :email, :value => "product@jombay.com"

  .actions
    = submit_tag 'Download'
