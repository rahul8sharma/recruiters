.inner
  = render :partial => "shared/report_header"      

  .scores.miscellaneous_factors
    .blue_bg_heading.pull-left
      %b Miscellaneous Factors
    .clr
    .left-offset3.pull-left
      %b Measured
    .left-offset1.pull-left
      %b Preferred
    .clr
    .divider1
    %ul.unstyled.miscellaneous_listing
      -3.times do 
        %li
          .span1.first.hexagon-icon.mid_favorable
          //use ".mid_favorable" or ".less_favorable" or ".favorable" to change the color of hexagon
          .span2.mid_favorable.pull-left
            %b Motivator
          .measured.pull-left
            Manipulative, Avoidance, Submissive, Negotiating, Competitive
          .preferred.pull-left
            %b.mid_favorable
              Avoidance
          .description.pull-left
            More likely to avoid confrontation with the other parties involved in conflict
          .clr
  = render :partial => "shared/report_footer", :locals => { :current_page => 6 }      

.page-break                    
