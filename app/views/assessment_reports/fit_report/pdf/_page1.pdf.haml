.inner{ style: "margin-top:0px;" }
  .overall_report
    .j-logo.pull-left
    - if @report.report_hash[:assessment][:brand_partner]
      .pull-right
        - imagePath = asset_path("brand-partners/"+@report.report_hash[:assessment][:brand_partner].underscore)
        .partner-logo{:style => "background: url(#{imagePath}.png) no-repeat center 0; width: 200px; height: 200px "}
    .clr
    .divider30
    .huge-text
      = @report.report_hash[:candidate][:name]
    .divider2
    - section_value.each do |section|
      = render :partial => section["path"]