%ul.training-need-bar-graph
  - counter = step
  - factor_scores.each_with_index do |(factor_name,factor_score), index|
    - counter = counter + 1
    %li
      .trait-title
        = factor_name
      .score
        #{factor_score[:percentage_users]}% Less Favourable
      .bar-graph
        .bar{:style=>"width: #{factor_score[:percentage_users]}%;"}
      .clr
.divider2