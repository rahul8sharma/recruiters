.competency_report
  .overview
    .bottom-triangle
      .container
        .divider1
        .content
          .overview-title
            .pull-left
              .divider3
              Overview of Competencies / Values Measured
            .scale.right-offset1
              .line
              .marker-container
                -#refer a code for scale from assessment_reports/competency_reports/html/competency_report_page_1.html.haml
                .marker
                  .arrow
                  .marker-info
                    sfdsfdfsdffds
                  high
                .clr
            .clr
          .divider2
          %ul.competency-list.pull-left
            %li
              .competency-name.pull-left
                competency_name
              .competency-value.pull-right
                -#refer a code for scale from assessment_reports/competency_reports/html/competency_report_page_1.html.haml
                .scale.right-offset1
                  .line
                  .marker-container
                    .marker{ class: "active" }
                      High
                    .clr
                .clr
                -#= competency_score[:grade]
              .clr
          .clr
        .divider2
