.overview
  .inner{ style: "margin-top:0px;" }
    .divider2
    .j-logo
    .divider4
    .large-text
      = @report.report_hash[:candidate][:name]
    .divider1
    .content.overview_scale
      .pull-left
        .overview-title.text-left
          Overview of Competencies / Values Measured
          .instruction_container
            .instructions
              .box.pull-left
                .box-icon.pull-left
                .box-title.pull-left Expected Score
                .clr
              .instruction1.pull-left
                Favorable
              .instruction2.less_favorable.pull-left
                Less Favorable
              .clr
      .scale
        .line
        .marker-container
          - @company_norm_buckets.each do |norm_bucket|
            .marker
              - if norm_bucket.name.split(" ").size == 1
                %br/
              = norm_bucket.name
          .clr
      .clr
      - if !@report.report_data[:show_favourable_above_desired_range]
        = render :partial => "assessment_reports/competency_report/pdf/custom_desired_range", :locals => { competency_scores: competency_scores }
      -else
        = render :partial => "assessment_reports/competency_report/pdf/generic_desired_range", :locals => { competency_scores: competency_scores }

    .divider2